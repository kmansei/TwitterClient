<% @tweets.each do |tweet| %>
  <div>
    <%= link_to image_tag(tweet.user.profile_image_url_https) %>
    <%= tweet.user.name %>
    <%= tweet.user.screen_name %><%= tweet.user.id %>
  </div>
  <div><%= tweet.text %></div>
  <div>RT <%= tweet.retweet_count %> いいね <%= tweet.favorite_count %></div>
  <div><%= tweet.created_at %></div>
  <% if tweet.media? %>
    <% tweet.media.each do |media| %>
      <%= image_tag(media.media_url_https) %>
    <% end %>
  <% end %>
<% end %>
